{{page-title "MarkerclustererIssue17"}}

<GMap
  @center={{this.center}}
  @disableDefaultUI={{true}}
  @zoomControl={{true}}
  @zoomControlOptions={{hash position=this.google.maps.ControlPosition.TOP_RIGHT}}
  @zoom={{12}}
  @onceOnIdle={{this.mapReady}}
  class="h-100"
  as |map|>
  <map.markerClusterer as |cluster|>

    {{#each this.markers as |location|}}
      <cluster.marker @lat={{location.lat}} @lng={{location.lng}} />
    {{/each}}

  </map.markerClusterer>
</GMap>